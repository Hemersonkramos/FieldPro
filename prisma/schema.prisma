generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Solicitacao {
  id          Int      @id @default(autoincrement())
  solicitacao String
  cliente     String
  regional    String
  municipio   String
  detalhes    String
  prazo       String
  lat         Float
  lng         Float
  emergencial Boolean
  equipe      String
  status      String
  createdAt   DateTime @default(now())
}
